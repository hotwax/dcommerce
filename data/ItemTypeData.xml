    <moqui.basic.Enumeration description="Discount - Supplier" enumId="ItemSupplierDiscount" enumTypeId="ItemType"/>
    <moqui.basic.Enumeration description="Fee" enumId="ItemFee" enumTypeId="ItemType"/>
